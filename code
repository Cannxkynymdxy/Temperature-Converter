check = input("If you want to convert fahrenheit to celsius write fah else write cel. ")
degree = float(input("Enter the degree you want to convert. "))
if check == "fah":
    print(f" {degree} degrees fahrenheit is {(degree - 32) * 5 / 9} degrees celsius.")
elif check == "cel":
    print(f" {degree} degrees celsius is {degree * 9 / 5 + 32} degrees fahrenheit.")
